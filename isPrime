import java.util.*;
 public class Main {
    public static void main(String[] args) {
        Scanner in = new Scanner(System.in);
        int n = in.nextInt();
        boolean check = isPrime(n);
        System.out.println(check);
        }
      
    static boolean isPrime(int n){
            if(n<=1){
                return false;
                
            }
            int c = 2;
            while(c * c <= n){
                if(n % c == 0){
                    return false;
                }
                c++;
            }
         }
            
    }
