import java.util.*;
 public class Main {
    public static void main(String[] args) {
        int[] nums = {1,5,34,35,5};
        System.out.println(Arrays.toString(nums));
        change(nums);
        System.out.println(Arrays.toString(nums));
         }
          static void change(int[] arr){
              arr[0] = 69;
          }   
    }
