import java.util.*;

public class Main {
    public static void main(String[] args) {
      int arr[]={1,3,5,7,8,9,66};
      int target = 7;
      System.out.println(linearSearching(arr,target,1,4));
      
  }
  static int linearSearching(int[] arr,int target,int start,int end){
    if(arr.length==0){
      return -1;
    }
    for(int i = start;i<=end;i++){
      int element = arr[i];
      if(element==target){
        return i;
      }
      
    }
    return -1;
  }
}
